{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "npm run vercel-build",
  "outputDirectory": "apps/plasma-venmo/.next",
  "installCommand": "npm install",
  "framework": "nextjs",
  "ignoreCommand": "git diff --quiet HEAD^ HEAD ./apps/plasma-venmo ./packages",
  "env": {
    "NEXT_PUBLIC_PLASMA_CHAIN_ID": "9745",
    "NEXT_PUBLIC_PLASMA_RPC": "https://rpc.plasma.to"
  },
  "regions": ["iad1"],
  "functions": {
    "apps/plasma-venmo/src/app/api/**/*.ts": {
      "maxDuration": 30
    }
  }
}
